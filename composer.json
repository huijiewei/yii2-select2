{
  "name": "huijiewei/yii2-select2",
  "description": "Select2 is a jQuery-based replacement for select boxes. It supports searching, remote data sets, and pagination of results.",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "yii2-extension",
  "homepage": "https://github.com/huijiewei/yii2-select2.git",
  "keywords": [
    "yii2",
    "extension",
    "select2",
    "assets"
  ],
  "authors": [
    {
      "name": "Huijie Wei",
      "email": "huijiewei@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "yiisoft/yii2": "^2.0",
    "huijiewei/yii2-fontawesome": "^2.0",
    "npm-asset/select2": "^4.0",
    "npm-asset/select2-bootstrap-theme": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "huijiewei\\select2\\": "src"
    }
  },
  "repositories": {
    "asset": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  }
}